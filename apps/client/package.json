    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook -o ./.storybook/static",
    "sb:docs": "start-storybook -p 6006 --docs",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
